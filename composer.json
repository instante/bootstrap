{
    "name": "instante/bootstrap",
    "type": "library",
    "description": "User-friendly interface to bootstrap Nette application",
    "keywords": [
        "nette",
        "instante"
    ],
    "license": [
        "BSD-3-Clause",
        "GPL-2.0",
        "GPL-3.0"
    ],
    "require": {
        "nette/bootstrap": "~2.4",
        "nette/robot-loader": "~2.4",
        "instante/utils": "@dev"
    },
    "require-dev": {
        "nette/tester": "@dev",
        "jakub-onderka/php-parallel-lint": "~0.6",
        "tracy/tracy": "@dev"
    },
    "suggest": {
        "instante/utils": "to simplify file path processing and stuff",
        "tracy/tracy": "to enable tracy debugging",
        "nette/application": "to make bootstrap working with nette 2.2 configurator"
    },
    "autoload": {
        "psr-4": {
            "Instante\\Bootstrap\\": "src/"
        }
    }
}
